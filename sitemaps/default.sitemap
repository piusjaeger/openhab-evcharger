sitemap default label="evcharger" {
	Frame label="Solar Power" {
		Text item=FroniusPV label="Solar power [%.0f W]" icon="solarplant" {
		Switch item=Chart_Period mappings=[0="H",1="D",2="W",3="M",4="Y"]
